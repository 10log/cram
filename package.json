{
  "name": "cram.ui",
  "version": "0.2.1",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/gregzanch/cram.git"
  },
  "description": "interface for computational room acoustics",
  "keywords": [
    "acoustics",
    "computation",
    "ray-tracing",
    "image-source",
    "fdtd"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./src/lib/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./src/lib/index.ts"
    },
    "./styles": {
      "import": "./dist/styles.js",
      "types": "./src/css/index.ts"
    },
    "./dist/cram.ui.css": "./dist/cram.ui.css"
  },
  "files": [
    "dist",
    "src/lib",
    "src/css",
    "src/store",
    "src/objects",
    "src/compute"
  ],
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "prebuild:jscad": "node scripts/prebundle-jscad.mjs",
    "build:lib": "npm run prebuild:jscad && vite build --mode lib",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:perf": "vitest run --testNamePattern=perf",
    "test:perf:update-baseline": "cross-env UPDATE_PERF_BASELINE=1 npm run test:perf",
    "lint": "eslint src/",
    "lint:types": "tsc --noEmit",
    "test:e2e": "npx playwright test",
    "test:e2e:update": "npx playwright test --update-snapshots",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:report": "npx playwright show-report"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@blueprintjs/core": "^6.4.1",
    "@blueprintjs/icons": "^6.3.2",
    "@blueprintjs/select": "^6.0.10",
    "@blueprintjs/table": "^6.0.10",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/x-tree-view": "^8.22.0",
    "@types/d3": "^7.4.3",
    "@types/plotly.js": "^3.0.9",
    "@types/zip.js": "^2.0.28",
    "@visx/axis": "^3.12.0",
    "@visx/gradient": "^3.12.0",
    "@visx/grid": "^3.12.0",
    "@visx/group": "^3.12.0",
    "@visx/legend": "^3.12.0",
    "@visx/responsive": "^3.12.0",
    "@visx/scale": "^3.12.0",
    "@visx/shape": "^3.12.0",
    "@visx/zoom": "^3.12.0",
    "@zip.js/zip.js": "^2.2.23",
    "ambisonics": "github:10log/TSAmbisonics",
    "beam-trace": "github:10log/BeamTrace2D#master",
    "chroma-js": "^3.2.0",
    "complex": "^3.0.1",
    "d3": "^7.9.0",
    "dxf-parser": "^1.0.0-alpha.2",
    "es-abstract": "^1.24.1",
    "fast-fuzzy": "^1.9.1",
    "fast-sort": "^3.4.1",
    "file-saver": "^2.0.2",
    "hotkeys-js": "^3.13.15",
    "immer": "^11.1.0",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "object-hash": "^3.0.0",
    "plotly.js": "^3.3.1",
    "react-app-polyfill": "^3.0.0",
    "react-color": "^2.18.0",
    "react-dev-utils": "^12.0.1",
    "react-plotly.js": "^2.6.0",
    "react-select": "^5.10.2",
    "react-tabs": "^6.1.0",
    "react-window": "^2.2.3",
    "semver": "^7.6.3",
    "simple-statistics": "^7.1.0",
    "three": "^0.181.2",
    "three-mesh-bvh": "^0.9.6",
    "three.meshline": "^1.4.0",
    "uplot": "^1.0.8",
    "uuid": "^13.0.0",
    "zustand": "^5.0.9"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@jscad/modeling": "^2.12.6",
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/chroma-js": "^3.1.2",
    "@types/file-saver": "^2.0.1",
    "@types/lodash": "^4.17.21",
    "@types/mathjs": "^6.0.12",
    "@types/node": "^24.10.4",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^18.3.27",
    "@types/react-color": "^3.0.1",
    "@types/react-dom": "^18.3.7",
    "@types/react-select": "^5.0.0",
    "@types/recharts": "^1.8.5",
    "@types/semver": "^7.3.1",
    "@types/three": "^0.181.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "babel-preset-react-app": "^10.0.0",
    "chalk": "^4.1.2",
    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "esbuild": "^0.27.2",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-unused-imports": "^4.3.0",
    "fast-check": "^4.5.0",
    "fs-extra": "^11.3.3",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.31",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-is": "^18.3.1",
    "resolve": "^1.22.11",
    "typescript": "^5.3.3",
    "vite": "^6.2.0",
    "vitest": "^4.0.17"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "unused-imports"
    ],
    "rules": {
      "no-unused-expressions": "off",
      "no-restricted-globals": "off",
      "@typescript-eslint/no-unused-expressions": "off",
      "import/no-anonymous-default-export": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "no-unused-vars": "off",
      "unused-imports/no-unused-imports": "warn",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  }
}
